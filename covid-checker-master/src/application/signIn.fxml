<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="351.0" prefWidth="352.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <children>
      <ImageView fitHeight="353.0" fitWidth="383.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../signinbackground.jpg" />
         </image>
      </ImageView>
      <Button layoutX="265.0" layoutY="240.0" mnemonicParsing="false" onAction="#goToSurveyPage" style="-fx-background-color: linear-gradient(#61a2b1, #2A5058);" text="Sign In" textFill="WHITE">
         <font>
            <Font name="Arial Narrow Bold" size="13.0" />
         </font>
         <effect>
            <DropShadow color="#00000099" />
         </effect></Button>
      <Label layoutX="64.0" layoutY="62.0" text="Welcome" textFill="#818181">
         <font>
            <Font name="Arial Black" size="32.0" />
         </font>
         <effect>
            <InnerShadow color="#000000e3" />
         </effect>
      </Label>
      <Label layoutX="63.0" layoutY="148.0" text="Username:" textFill="#333333">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <DropShadow blurType="GAUSSIAN" color="#ffffff80" />
         </effect>
      </Label>
      <Label layoutX="65.0" layoutY="197.0" text="Password:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <DropShadow blurType="GAUSSIAN" color="#ffffff80" />
         </effect>
      </Label>
      <TextField fx:id="userNameText" layoutX="158.0" layoutY="145.0" prefHeight="22.0" prefWidth="161.0" />
      <TextField fx:id="passwordText" layoutX="158.0" layoutY="194.0" prefHeight="22.0" prefWidth="161.0" />
   </children>
</AnchorPane>
