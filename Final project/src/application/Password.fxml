<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="292.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PasswordController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="518.0" opacity="0.29" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../mainBackground.jpg" />
         </image>
      </ImageView>
      <Label layoutX="147.0" layoutY="49.0" text="Welcome" textFill="#818181">
         <font>
            <Font name="Arial Black" size="32.0" />
         </font>
         <effect>
            <InnerShadow color="#000000e3" />
         </effect>
      </Label>
      <Label layoutX="147.0" layoutY="136.0" text="Password:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <DropShadow blurType="GAUSSIAN" color="#ffffff80" />
         </effect>
      </Label>
      <PasswordField fx:id="password" layoutX="231.0" layoutY="130.0" />
      <Button layoutX="322.0" layoutY="189.0" mnemonicParsing="false" onAction="#loginButton" text="Sign in">
         <effect>
            <DropShadow />
         </effect></Button>
      <ImageView fitHeight="69.0" fitWidth="120.0" opacity="0.68" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../backbtn-removebg-preview.png" />
         </image>
      </ImageView>
      <Button layoutX="-1.0" layoutY="-5.0" mnemonicParsing="false" onAction="#goBack" opacity="0.0" prefHeight="79.0" prefWidth="120.0" text="Button" />
   </children>
</AnchorPane>
