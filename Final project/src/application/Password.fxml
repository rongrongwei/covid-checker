<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="292.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PasswordController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="518.0" opacity="0.29" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../mainBackground.jpg" />
         </image>
      </ImageView>
      <Label layoutX="184.0" layoutY="45.0" text="Welcome" textFill="#818181">
         <font>
            <Font name="Arial Black" size="32.0" />
         </font>
         <effect>
            <InnerShadow color="#000000e3" />
         </effect>
      </Label>
      <Label layoutX="109.0" layoutY="134.0" text="Password:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <DropShadow blurType="GAUSSIAN" color="#ffffff80" />
         </effect>
      </Label>
      <PasswordField fx:id="password" layoutX="193.0" layoutY="128.0" />
      <Button layoutX="237.0" layoutY="186.0" mnemonicParsing="false" onAction="#loginButton" text="Sign in">
         <effect>
            <DropShadow />
         </effect></Button>
   </children>
</AnchorPane>
